{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": true,
    "no-submodule-imports": false,
    "no-unused-expression": false
  }
}
